<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/internet_container" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:id="@+id/wifi_list" android:layout_height="@dimen/internet_dialog_wifi_network_height" android:paddingStart="20dp" android:paddingEnd="24dp" style="@style/InternetDialog.Network">
        <FrameLayout android:layout_gravity="left|center_vertical|center_horizontal|center|start" android:clickable="false" android:layout_width="24dp" android:layout_height="24dp">
            <ImageView android:layout_gravity="center" android:id="@+id/wifi_icon" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </FrameLayout>
        <LinearLayout android:gravity="left|center_vertical|center_horizontal|center|start" android:orientation="vertical" android:id="@+id/wifi_network_layout" android:clickable="false" android:layout_width="wrap_content" android:layout_height="@dimen/internet_dialog_wifi_network_height" android:layout_weight="1" android:layout_marginEnd="30dp">
            <TextView android:textSize="14sp" android:id="@+id/wifi_title" style="@style/InternetDialog.NetworkTitle"/>
            <TextView android:id="@+id/wifi_summary" style="@style/InternetDialog.NetworkSummary"/>
        </LinearLayout>
        <FrameLayout android:layout_gravity="right|center_vertical|center_horizontal|center|end" android:clickable="false" android:layout_width="24dp" android:layout_height="match_parent">
            <ImageView android:layout_gravity="right|center_vertical|center_horizontal|center|end" android:id="@+id/wifi_end_icon" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        </FrameLayout>
    </LinearLayout>
</LinearLayout>
