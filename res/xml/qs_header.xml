<?xml version="1.0" encoding="utf-8"?>
<ConstraintSet xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:systemui="http://schemas.android.com/apk/res/com.android.systemui" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/qs_header_constraint">
    <Constraint android:id="@+id/clock">
        <Layout android:layout_width="wrap_content" android:layout_height="48dp" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintEnd_toStartOf="@+id/carrier_group" app:layout_constraintHorizontal_bias="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/date"/>
    </Constraint>
    <Constraint android:id="@+id/date">
        <Layout android:layout_width="wrap_content" android:layout_height="48dp" app:layout_constraintBottom_toTopOf="@+id/clock" app:layout_constraintEnd_toEndOf="0" app:layout_constraintHorizontal_bias="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="0"/>
        <Motion app:motionStagger="0.5"/>
    </Constraint>
    <Constraint android:id="@+id/carrier_group">
        <CustomAttribute app:attributeName="alpha" app:customFloatValue="1"/>
    </Constraint>
    <Constraint android:id="@+id/privacy_container">
        <Layout android:layout_width="wrap_content" android:layout_height="48dp" app:layout_constraintBottom_toBottomOf="@+id/date" app:layout_constraintEnd_toEndOf="0" app:layout_constraintTop_toTopOf="@+id/date"/>
    </Constraint>
</ConstraintSet>
